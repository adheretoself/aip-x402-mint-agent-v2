{
  "version": 2,
  "installCommand": "npm install --production",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": ".well-known/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/.well-known/(.*)",
      "dest": "/.well-known/$1"
    },
    {
      "src": "/.*",
      "dest": "index.js"
    }
  ]
}
